{
  "name": "review-app",
  "version": "0.0.0",
  "license": "ISC",
  "description": "App for fetching the series data",
  "scripts": {
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "nodemon",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "coverage": "nyc --reporter html --reporter text npm test"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.1.0",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.47",
    "@types/jasmine": "^3.5.12",
    "@types/mocha": "^8.0.2",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "chai": "^4.2.0",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jsonfile": "^6.0.1",
    "mocha": "^8.1.1",
    "mocha-simple-html-reporter": "^1.1.0",
    "node-mocks-http": "^1.8.1",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "sinon": "^9.0.3",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "ts-sinon": "^1.2.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
